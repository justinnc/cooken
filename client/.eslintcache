)\n            .then(result => {\n                result = result.map((obj) => {\n                    return obj.name\n                }).slice(0, 3)\n\n                setSuggestions(result)\n            })\n            .catch(err => console.log(err))\n    }, [searchInput])\n\n    useEffect(() => {\n        addIngredient(searchInputFinal)\n        setSearchInput('')\n        setCursor(0)\n    }, [searchInputFinal])\n\n    return (\n        <div\n            className={\"focus:outline relative shadow-md rounded-lg flex-grow \" + (props.className || \"\")}\n        >\n            <span className=\"absolute inset-y-0 left-0 pl-2 top-1\">\n                <button type=\"submit\" className=\"p-1 focus:outline-none focus:shadow-outline\">\n                    <img src=\"add-icon.svg\" alt=\"add\" />\n                </button>\n            </span>\n            <input\n                className={\"focus:outline-none py-2 pl-8 text-center text-sm border-solid w-full border-b\"}\n                type=\"search\"\n                placeholder=\"add an ingredient...\"\n                value={searchInput}\n                onChange={handleChange}\n                onKeyDown={handleKeyDown}\n            />\n            <ul className=\"relative right-0 mx-auto text-sm text-center text-gray-800 bg-white\">\n                {suggestions.map((suggestion, idx) => {\n                    let className = (cursor === idx ? 'bg-gray-50 ' : null) + \"block w-full px-1 pt-1 pl-4 \"\n\n                    if (idx === suggestions.length - 1) {\n                        className += \"pb-2\"\n                    } else {\n                        className += \"pb-1\"\n                    }\n\n                    return <li key={idx} className={className}>{suggestion}</li>\n                })}\n            </ul>\n        </div>\n    )\n}\n\nexport default SearchBar\n","/home/risan/Desktop/Projects/cooken/client/src/helpers/index.js",[],"/home/risan/Desktop/Projects/cooken/client/src/helpers/capitali